<div class="container">
  <div class="row justify-content-center">
    <div class="col-10 mt-5">
      <h2><i class="fas fa-users" %></i>ユーザー一覧</h2>
        <table class="table text-center">
        <% @users.each do |user| %>
          <tr>
            <td>
              <%= link_to user_path(user) do %>
                <%= user.name %>
              <% end %>
            </td>
          </tr>
        <% end %>
        </table>
    </div>
  </div>
</div>